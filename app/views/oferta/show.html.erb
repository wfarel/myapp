<div class="container margen text-center">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Titulo:</strong>
    <%= @ofertum.titulo %>
  </p>

  <p>
    <strong>Codigo:</strong>
    <%= @ofertum.codigo %>
  </p>

  <p>
    <%= image_tag @ofertum.imagen.url(:medium) %>
  </p>

  <p>
    <strong>Descripcion:</strong>
    <%= @ofertum.descripcion %>
  </p>

  <p>
    <strong>Precio:</strong>
    <%= @ofertum.precio %>
  </p>

  <%= link_to 'Edit', edit_ofertum_path(@ofertum) %> |
  <%= link_to 'Back', oferta_path %>

</div>
