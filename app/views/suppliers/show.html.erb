<div class="container margen text-center">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Nombre:</strong>
    <%= @supplier.nombre %>
  </p>

  <p>
    <strong>Industria:</strong>
    <%= @supplier.industria %>
