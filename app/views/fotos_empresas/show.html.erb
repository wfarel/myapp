<div class="container margen text-center">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Name:</strong>
    <%= @fotos_empresa.name %>
  </p>

  <p>
    <%= image_tag @fotos_empresa.image.url(:medium) %>
  </p>

  <p>
    <strong>Estado:</strong>
    <%= @fotos_empresa.estado %>
  </p>

  <%= link_to 'Edit', edit_fotos_empresa_path(@fotos_empresa) %> |
  <%= link_to 'Back', fotos_empresas_path %>
</div>
